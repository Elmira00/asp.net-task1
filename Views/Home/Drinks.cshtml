@{
}
@model asp.net_task1.Models.DrinksViewModel
<div class="text-center">
    <h1 style="color:rebeccapurple">Drinks</h1>
    @foreach (var item in Model.Drinks)
    {
        <section style="width:20%;margin:2%;display:inline-block">
            <h5>Name : @item.Name</h5>
            <h5>Category : @item.Category</h5>
            <h5>Is Alcoholic : @item.IsAlcoholic</h5>
            <h5>Packaging : @item.Packaging</h5>
            <h5>Volume : @item.Volume</h5>
            <h5>Calories : @item.Calories</h5>
            <h5>Price : @item.Price</h5>
        </section>
    }
   

</div>